    "version": "0.2.0",
    "configurations": [
        {
            "name": "obama (debug mode)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "flutterMode": "debug",
            "args": [
                "--web-port", "4040",
                "--web-browser-flag", "--disable-web-security"
            ]
        },
        {
            "name": "obamaHome (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "obamaHome (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}